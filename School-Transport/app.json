{
  "expo": {
    "name": "Octagon-Uncle-Transport",
    "slug": "Octagon-Uncle-Transport",
    "scheme": "octagonuncletransport",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "experiments": {
      "typedRoutes": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.octagonuncletransportsolutions.outs"
    },
    "android": {
      "package": "com.yourcompany.octagonuncletransport",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "49.0.0",
    "plugins": [
      "expo-router"
    ]
  },
  "name": "Octagon-Uncle-Transport"
}
